{
  "name": "greatvision-wx-web-pc",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env ENV=loc NODE_ENV=development webpack serve",
    "cleandist": "rm -rf ./dist",
    "cleandll": "rm -rf ./dll",
    "devDebugNode": "cross-env ENV=loc NODE_ENV=development NODE_OPTIONS='--inspect' webpack serve",
    "deploy-dev": "npm run dll && cross-env ENV=dev NODE_ENV=production webpack",
    "deploy-test": "npm run dll && cross-env ENV=test NODE_ENV=production webpack",
    "deploy-uat": "npm run dll && cross-env ENV=uat NODE_ENV=production webpack",
    "deploy-prod": "npm run dll && cross-env ENV=prod NODE_ENV=production webpack",
    "dll": "npm run cleandll && webpack --config=webpack.dll.config.js"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "ali-oss": "^6.17.1",
    "antd-mobile": "^5.24.0",
