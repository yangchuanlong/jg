    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "mobx": "^6.6.1",
    "mobx-react": "^7.5.2",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-perfect-scrollbar": "^1.5.8",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
